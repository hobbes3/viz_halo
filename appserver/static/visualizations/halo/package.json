  "devDependencies": {
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "d3": "^5.7.0",
    "d3-scale-chromatic": "^1.3.3",
    "jquery": "^3.3.1",
    "underscore": "^1.9.1"
  }
}
